{
  "name": "fast-brake",
  "version": "0.0.1",
  "description": "Blazing fast ES feature detection with zero runtime dependencies. Analyzes JavaScript code to detect ECMAScript features and verify compatibility with target ES versions using a revolutionary two-phase detection system.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "bun generator/generate-schema.ts",
    "build": "turbo run build-dist",
    "build-dist": "bun run clean-dist && tsup src/index.ts --format esm,cjs --dts --minify",
    "clean-dist": "rimraf dist",
    "commit": "git-cz",
    "commit-msg": "commitlint --edit $1",
    "dev": "bun run --cwd site dev",
    "lint": "oxlint src tests",
    "lint-fix": "oxlint src tests --fix",
    "lint-src": "oxlint src",
    "pre-commit": "lint-staged --quiet && bun run test",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "bun run test && bun run build",
    "preview": "bun run --cwd site preview",
    "release": "release-it",
    "test": "bun test src/*.test.ts tests/index.test.ts --timeout 5000",
    "test:all": "bun test src/*.test.ts tests/*.test.ts --timeout 5000",
    "test:unit": "bun test src/*.test.ts --timeout 5000",
    "test:integration": "bun test tests/index.test.ts --timeout 5000",
    "test:e2e": "bun test tests/e2e.test.ts --timeout 5000",
    "test-e2e": "bun test tests/e2e.test.ts --timeout 5000",
    "test:watch": "bun test --watch src/*.test.ts tests/*.test.ts",
    "benchmark": "bun tests/benchmark.ts",
    "benchmark:quick": "bun tests/benchmark.ts --quick",
    "typecheck": "turbo run typecheck-src",
    "typecheck-src": "tsc --noEmit",
    "update": "codependence --update && bun run update-site-deps",
    "update-site-deps": "cd site && bun update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/fast-brake.git"
  },
  "keywords": [
    "es",
    "ecmascript",
    "feature-detection",
    "compatibility",
    "javascript",
    "typescript",
    "ast",
    "parser",
    "tokenizer",
    "babel",
    "transpiler",
    "build-tools",
    "webpack",
    "rollup",
    "vite",
    "eslint",
    "performance",
    "fast",
    "zero-dependencies",
    "es5",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "es2019",
    "es2020",
    "es2021",
    "es2022",
    "arrow-functions",
    "template-literals",
    "async-await",
    "optional-chaining",
    "nullish-coalescing",
    "private-fields",
    "static-blocks",
    "bigint",
    "classes",
    "destructuring",
    "spread-operator",
    "for-of",
    "generators",
    "promises",
    "modules",
    "import",
    "export"
  ],
  "author": "Jeff Wainwright <https:jeffry.in> (yowainwright@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yowainwright/fast-brake/issues"
  },
  "homepage": "https://jeffry.in/fast-brake/",
  "workspaces": [
    "site"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/format": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "24.1.0",
    "codependence": "^0.3.1",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "16.1.2",
    "oxlint": "^1.11.2",
    "path-exists-cli": "^2.0.0",
    "prettier": "3.6.2",
    "release-it": "19.0.4",
    "rimraf": "6.0.1",
    "stdouttojson": "1.0.0",
    "tslib": "2.8.1",
    "tsup": "8.5.0",
    "tsx": "^4.19.2",
    "turbo": "2.5.5",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.12.2",
    "vite": "7.0.6"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "oxlint --fix"
    ],
    "tests/**/*.ts": [
      "oxlint --fix"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release"
    }
  },
  "packageManager": "bun@1.1.38"
}
