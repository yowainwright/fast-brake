{
  "name": "fast-brake",
  "version": "0.1.0",
  "description": "fast js feature detection with zero runtime dependencies; analyzes js to verify compatibility",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "bun utils/generator/src/generate-schema.ts",
    "build": "turbo run build:pkg",
    "build:pkg": "bun run clean && bun run build:run",
    "build:run": "tsup src/index.ts --format esm,cjs --dts --minify",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "commit-msg": "commitlint --edit $1",
    "dev": "cd site && bun run dev",
    "lint": "turbo run lint:run",
    "lint:run": "oxlint src tests --ignore-pattern='tests/fixtures/*'",
    "lint:fix": "oxlint src tests --fix",
    "pre-commit": "lint-staged --quiet && bun run test",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "bun run test && bun run build",
    "preview": "bun run --cwd site preview",
    "release": "release-it",
    "test": "turbo run test:run",
    "test:run": "bun test tests/unit tests/integration/integration.test.ts --timeout 5000",
    "test:all": "bun test tests/**/*.test.ts --timeout 5000",
    "test:unit": "turbo run test:unit",
    "test:unit:run": "bun test tests/unit/*.test.ts --timeout 5000",
    "test:integration": "turbo run test:integration",
    "test:integration:run": "bun test tests/integration/integration.test.ts --timeout 5000",
    "test:watch": "bun test --watch tests/**/*.test.ts",
    "benchmark": "turbo run benchmark",
    "benchmark:run": "bun utils/benchmarker/src/benchmark.ts",
    "benchmark:quick": "bun tests/integration/benchmarks.ts --quick",
    "benchmark:update": "cd utils/benchmarker && bun run benchmark:all",
    "typecheck": "turbo run typecheck",
    "typecheck:run": "tsc --noEmit",
    "update": "codependence --update && bun run update-site-deps",
    "update-site-deps": "cd site && bun update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/fast-brake.git"
  },
  "keywords": [
    "es",
    "ecmascript",
    "feature-detection",
    "compatibility",
    "javascript",
    "typescript",
    "ast",
    "parser",
    "tokenizer",
    "babel",
    "transpiler",
    "build-tools",
    "webpack",
    "rollup",
    "vite",
    "eslint",
    "performance",
    "fast",
    "zero-dependencies",
    "es5",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "es2019",
    "es2020",
    "es2021",
    "es2022",
    "arrow-functions",
    "template-literals",
    "async-await",
    "optional-chaining",
    "nullish-coalescing",
    "private-fields",
    "static-blocks",
    "bigint",
    "classes",
    "destructuring",
    "spread-operator",
    "for-of",
    "generators",
    "promises",
    "modules",
    "import",
    "export"
  ],
  "author": "Jeff Wainwright <https:jeffry.in> (yowainwright@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yowainwright/fast-brake/issues"
  },
  "homepage": "https://jeffry.in/fast-brake/",
  "workspaces": [
    ".",
    "site",
    "utils/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/node": "24.1.0",
    "codependence": "^0.3.1",
    "husky": "9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "16.1.2",
    "oxlint": "^1.11.2",
    "prettier": "3.6.2",
    "release-it": "19.0.4",
    "rimraf": "6.0.1",
    "tslib": "2.8.1",
    "tsup": "8.5.0",
    "turbo": "2.5.5",
    "typescript": "5.8.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "oxlint --fix"
    ],
    "tests/**/*.ts": [
      "oxlint --fix"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release"
    }
  },
  "packageManager": "bun@1.1.38"
}
