{
  "name": "fast-brake",
  "version": "0.1.2",
  "description": "fast js feature detection with zero runtime dependencies; analyzes js to verify compatibility",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugins/es2015": {
      "types": "./dist/plugins/es2015/index.d.ts",
      "import": "./dist/plugins/es2015/index.js",
      "require": "./dist/plugins/es2015/index.cjs"
    },
    "./plugins/es2016": {
      "types": "./dist/plugins/es2016/index.d.ts",
      "import": "./dist/plugins/es2016/index.js",
      "require": "./dist/plugins/es2016/index.cjs"
    },
    "./plugins/es2017": {
      "types": "./dist/plugins/es2017/index.d.ts",
      "import": "./dist/plugins/es2017/index.js",
      "require": "./dist/plugins/es2017/index.cjs"
    },
    "./plugins/es2018": {
      "types": "./dist/plugins/es2018/index.d.ts",
      "import": "./dist/plugins/es2018/index.js",
      "require": "./dist/plugins/es2018/index.cjs"
    },
    "./plugins/es2019": {
      "types": "./dist/plugins/es2019/index.d.ts",
      "import": "./dist/plugins/es2019/index.js",
      "require": "./dist/plugins/es2019/index.cjs"
    },
    "./plugins/es2020": {
      "types": "./dist/plugins/es2020/index.d.ts",
      "import": "./dist/plugins/es2020/index.js",
      "require": "./dist/plugins/es2020/index.cjs"
    },
    "./plugins/es2021": {
      "types": "./dist/plugins/es2021/index.d.ts",
      "import": "./dist/plugins/es2021/index.js",
      "require": "./dist/plugins/es2021/index.cjs"
    },
    "./plugins/es2022": {
      "types": "./dist/plugins/es2022/index.d.ts",
      "import": "./dist/plugins/es2022/index.js",
      "require": "./dist/plugins/es2022/index.cjs"
    },
    "./plugins/es2023": {
      "types": "./dist/plugins/es2023/index.d.ts",
      "import": "./dist/plugins/es2023/index.js",
      "require": "./dist/plugins/es2023/index.cjs"
    },
    "./plugins/es2024": {
      "types": "./dist/plugins/es2024/index.d.ts",
      "import": "./dist/plugins/es2024/index.js",
      "require": "./dist/plugins/es2024/index.cjs"
    },
    "./plugins/es2025": {
      "types": "./dist/plugins/es2025/index.d.ts",
      "import": "./dist/plugins/es2025/index.js",
      "require": "./dist/plugins/es2025/index.cjs"
    },
    "./plugins/detect": {
      "types": "./dist/plugins/detect/index.d.ts",
      "import": "./dist/plugins/detect/index.js",
      "require": "./dist/plugins/detect/index.cjs"
    },
    "./plugins/all": {
      "types": "./dist/plugins/all/index.d.ts",
      "import": "./dist/plugins/all/index.js",
      "require": "./dist/plugins/all/index.cjs"
    },
    "./plugins/telemetry": {
      "types": "./dist/plugins/telemetry/index.d.ts",
      "import": "./dist/plugins/telemetry/index.js",
      "require": "./dist/plugins/telemetry/index.cjs"
    },
    "./plugins/browserlist": {
      "types": "./dist/plugins/browserlist/index.d.ts",
      "import": "./dist/plugins/browserlist/index.js",
      "require": "./dist/plugins/browserlist/index.cjs"
    },
    "./plugins/esversion": {
      "types": "./dist/plugins/esversion/index.d.ts",
      "import": "./dist/plugins/esversion/index.js",
      "require": "./dist/plugins/esversion/index.cjs"
    },
    "./extensions/loc": {
      "types": "./dist/extensions/loc.d.ts",
      "import": "./dist/extensions/loc.js",
      "require": "./dist/extensions/loc.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "bun utils/generator/src/generate-schema.ts",
    "build": "turbo run build:pkg",
    "build:pkg": "bun run clean && bun run build:run",
    "build:run": "tsup",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "commit-msg": "commitlint --edit $1",
    "dev": "cd site && bun run dev",
    "lint": "turbo run lint:run",
    "lint:run": "oxlint src tests --ignore-pattern='tests/fixtures/*'",
    "lint:fix": "oxlint src tests --fix",
    "format": "turbo run format:run",
    "format:run": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "pre-commit": "lint-staged --quiet && bun run test",
    "prepublishOnly": "bun run test && bun run build",
    "preview": "bun run --cwd site preview",
    "release": "release-it",
    "test": "turbo run test:run",
    "test:run": "bun test tests/unit tests/integration/integration.test.ts --timeout 5000",
    "test:all": "bun test tests/**/*.test.ts --timeout 5000",
    "test:unit": "turbo run test:unit",
    "test:unit:run": "bun test tests/unit/*.test.ts --timeout 5000",
    "test:integration": "turbo run test:integration",
    "test:integration:run": "bun test tests/integration/integration.test.ts --timeout 5000",
    "test:watch": "bun test --watch tests/**/*.test.ts",
    "benchmark": "bun run --cwd utils/benchmarker src/benchmark.ts",
    "benchmark:run": "bun utils/benchmarker/src/benchmark.ts",
    "benchmark:quick": "bun tests/integration/benchmarks.ts --quick",
    "benchmark:update": "bun run --cwd utils/benchmarker update-docs",
    "typecheck": "turbo run typecheck:run",
    "typecheck:run": "tsc --noEmit",
    "update": "codependence --update && bun install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/fast-brake.git"
  },
  "keywords": [
    "es",
    "ecmascript",
    "feature-detection",
    "compatibility",
    "javascript",
    "typescript",
    "ast",
    "parser",
    "tokenizer",
    "babel",
    "transpiler",
    "build-tools",
    "webpack",
    "rollup",
    "vite",
    "eslint",
    "performance",
    "fast",
    "zero-dependencies",
    "es5",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "es2019",
    "es2020",
    "es2021",
    "es2022",
    "arrow-functions",
    "template-literals",
    "async-await",
    "optional-chaining",
    "nullish-coalescing",
    "private-fields",
    "static-blocks",
    "bigint",
    "classes",
    "destructuring",
    "spread-operator",
    "for-of",
    "generators",
    "promises",
    "modules",
    "import",
    "export"
  ],
  "author": "Jeff Wainwright <https:jeffry.in> (yowainwright@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yowainwright/fast-brake/issues"
  },
  "homepage": "https://jeffry.in/fast-brake/",
  "workspaces": [
    ".",
    "site",
    "utils/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/node": "24.1.0",
    "codependence": "^1.0.0-1",
    "husky": "9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "16.1.2",
    "oxlint": "^1.12.0",
    "prettier": "3.6.2",
    "release-it": "19.0.4",
    "rimraf": "6.0.1",
    "tslib": "2.8.1",
    "tsup": "8.5.0",
    "turbo": "2.5.5",
    "typescript": "5.8.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "oxlint --fix",
      "prettier --write"
    ],
    "tests/**/*.ts": [
      "oxlint --fix",
      "prettier --write"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release"
    }
  },
  "packageManager": "bun@1.1.38"
}
