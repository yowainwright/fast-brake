{
  "name": "@fast-brake/benchmarker",
  "version": "0.0.1",
  "description": "Benchmarking suite for fast-brake",
  "type": "module",
  "private": true,
  "scripts": {
    "test:run": "bun test tests",
    "build:pkg": "echo 'Skipping benchmarker build (utility only)'",
    "clean": "rimraf dist",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src tests",
    "lint:fix": "oxlint src tests --fix",
    "benchmark": "bun run src/benchmark.ts",
    "benchmark:table": "bun run src/generate-table.ts",
    "benchmark:update": "bun run src/update-benchmarks.ts",
    "benchmark:all": "bun run benchmark && bun run benchmark:table && bun run benchmark:update"
  },
  "dependencies": {
    "fast-brake": "file:../..",
    "@babel/parser": "^7.26.3",
    "acorn": "^8.14.0",
    "esprima": "^4.0.1",
    "espree": "^10.3.0",
    "meriyah": "^6.0.2",
    "cherow": "^1.6.9",
    "browserslist": "^4.24.3",
    "caniuse-lite": "^1.0.30001690",
    "cli-table3": "^0.6.5",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "24.1.0",
    "@types/esprima": "^4.0.6",
    "@types/espree": "^10.1.0",
    "@types/bun": "latest",
    "oxlint": "^1.11.2",
    "prettier": "3.6.2",
    "typescript": "5.8.3"
  }
}