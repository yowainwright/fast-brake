{
  "name": "@fast-brake/generator",
  "version": "0.0.1",
  "description": "Schema generator for fast-brake",
  "type": "module",
  "private": true,
  "dependencies": {
    "fast-brake": "*"
  },
  "scripts": {
    "test": "bun test tests",
    "build": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src tests",
    "lint:fix": "oxlint src tests --fix",
    "generate": "bun src/generate-schema.ts"
  },
  "devDependencies": {
    "@types/node": "24.1.0",
    "@types/bun": "latest",
    "oxlint": "^1.11.2",
    "prettier": "3.6.2",
    "typescript": "5.8.3"
  }
}